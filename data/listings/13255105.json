.","weekly_price_factor":0.85,"weekly_price_native":null,"additional_house_rules":"A lot of effort has been put in the house to make this a nice, peaceful, clean and tidy  place. We expect our guests to treat the apartment in the same respectful way as we do.","in_building":false,"in_toto_area":false,"recent_review":null,"toto_opt_in":null,"wireless_info":null,"listing_occupancy_info":{"show_occupancy_message":false},"review_rating_accuracy":null,"review_rating_checkin":null,"review_rating_cleanliness":null,"review_rating_communication":null,"review_rating_location":null,"review_rating_value":null,"is_business_travel_ready":false}}