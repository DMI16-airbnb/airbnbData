kly_price_factor":0.9,"weekly_price_native":null,"additional_house_rules":"Amsterdam is a city of liberty, so no need for a long list of rules. Please just behave like you would hope guests in your house would do. This includes:\n\n- Keep down the noise at night. The house is characteristic and old, so sound carries;\n- Take care of our stuff; we've collected it quite lovingly;\n- No smoking please.\n\nIn short: common sense :)","in_building":false,"in_toto_area":false,"recent_review":null,"toto_opt_in":null,"wireless_info":null,"listing_occupancy_info":{"show_occupancy_message":false},"review_rating_accuracy":null,"review_rating_checkin":null,"review_rating_cleanliness":null,"review_rating_communication":null,"review_rating_location":null,"review_rating_value":null,"is_business_travel_ready":false}}